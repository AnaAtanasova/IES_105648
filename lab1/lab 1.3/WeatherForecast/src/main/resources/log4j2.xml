<Configuration status="debug" name="AnaLogger" packages="">
    <Appenders>
        <File name="AnaFileLogger" filename="logLab1.log">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %p %m%n"/>
        </File>
    </Appenders>
    <loggers>
        <root level="debug">
            <appender-ref ref="AnaFileLogger" level="info"/>
        </root>
    </loggers>
</Configuration>